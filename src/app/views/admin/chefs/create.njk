{% extends "admin/layout.njk" %}

{% set title = "Foodify - Admin - Chef" %}

{% block styles %}
  <link rel="stylesheet" href="/styles/admin/create.css">
{% endblock styles %}

{% block main %}

  <main>
    <form action="/admin/chefs" method='post'>
      <div class="head-form">
        <h3>Cadastrando Chef
        </h3>
        <button type="submit"><span>Salvar Chef</span></button>
      </div>
      <label for="name">Nome</label>
      <input type="text" name="name">
      <label for="avatar_url">Avatar</label>
      <input type="text" name="avatar_url">
    </form>
    

      
  </main>

{% endblock main %}
